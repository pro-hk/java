import java.util.Scanner;

public class GradeSwitch {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("점수를 입력하세요.");
		int score = scanner.nextInt();
		int result = score / 10;
		System.out.println(result);
		// 버림 == Math.floor
		scanner.close();
	}

}
