package Practice2;

import java.util.Scanner;

public class Practice2_3 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("금액을 입력하시오");
		int price = scanner.nextInt();
		int 오만원 = price / 50000;
		int 만원 = (price - 50000 * 오만원)/10000;
		int 천원 = (price - 50000 * 오만원 - 10000 * 만원)/1000;
		int 백원 = (price - 50000 * 오만원 - 10000 * 만원 - 1000 * 천원)/100;
		int 오십원 = (price - 50000 * 오만원 - 10000 * 만원 - 1000 * 천원 - 100*백원)/50;
		int 십원 = (price - 50000 * 오만원 - 10000 * 만원 - 1000 * 천원 - 100*백원 - 50*오십원)/10;
		int 일원 = price%10;
		if (price / 50000 >= 1) {
			System.out.println("오만원권 " + price / 50000 + "매");
			System.out.println("만원권 " + price - (price/50000)*50000 / 10000 + "매");
			System.out.println("천원권 " + price / 1000 + "매");
			System.out.println("오만원권 " + price / 50000 + "매");
			System.out.println("오만원권 " + price / 50000 + "매");
		} else if ()
	}
}