
public class chap0101 {

	public static void main(String[] args) {
		int num = 10;   // 정수
		long l = 1234l; // 정수 롱타입 : 끝에 l or L
		// 정수
		char c ='j';
		String str = "Hello";
		// 문자
		double d = 0.1234; // 실수
		float f = 0.1234f;  // float형 실수 : 끝에 f
		// 실수
		num = 015; // 0 : 8진수,  0x : 16진수 -- 10이상 abcdef, 0b : 2진수 
		boolean isActive = true;
		// t/f
		
		
		
		System.out.println(num);
	}
}
