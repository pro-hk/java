package practice;

import java.util.Scanner;

public class Ex07 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		while(true)
		System.out.print(">>");
		String dd = scanner.nextLine();
		if(dd.equals("그만")) {
			System.out.println("종료합니다...");
			break;
			}
	}
}
